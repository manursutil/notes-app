			"name": "NewNote",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"201 Created\", () => {",
							"  pm.response.to.have.status(201);",
							"});",
							"",
							"const createdNote = pm.response.json();",
							"pm.environment.set(\"noteId\", createdNote.id);",
							"pm.environment.set(\"noteTitle\", createdNote.title);",
							"pm.environment.set(\"noteContent\", createdNote.content);",
							"",
							"pm.test(\"Has title and content\", () => {",
							"  pm.expect(createdNote.title).to.exist;",
							"  pm.expect(createdNote.content).to.exist;",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
